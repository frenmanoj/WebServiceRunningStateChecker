  </welcome-file-list>
  
   <servlet>
        <servlet-name>WatchDogAgent</servlet-name>
        <servlet-class>com.mum.seminar.webservice.watchdog.WatchDogAgent</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>WatchDogAgent</servlet-name>
